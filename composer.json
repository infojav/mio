{
	"name": "javper/mio-project",
	"description": "Mio Project Template",
	"keywords": ["mio", "template"],
	"homepage": "https://github.com/javper/mio",
	"type": "project",
	"license": "",
	"support": {
		"issues": "https://github.com/javper/mio/issues?state=open"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"suggest": {
		"codeception/codeception": "Codeception, 2.0.* is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception",
		"yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"backend/runtime",
			"backend/web/assets",

			"frontend/runtime",
			"frontend/web/assets"
		]
	}
}
